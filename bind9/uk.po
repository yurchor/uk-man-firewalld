msgstr "Внутрішня помилка\\&."

#. type: SH
#: dig.1:820 host.1:259 nslookup.1:287
#, no-wrap
msgid "FILES"
msgstr "ФАЙЛИ"

#. type: Plain text
#: dig.1:823 host.1:262 nslookup.1:290
msgid "/etc/resolv\\&.conf"
msgstr "/etc/resolv\\&.conf"

#. type: Plain text
#: dig.1:825
msgid "${HOME}/\\&.digrc"
msgstr "${HOME}/\\&.digrc"

#. type: SH
#: dig.1:825 host.1:262 nslookup.1:290
#, no-wrap
msgid "SEE ALSO"
msgstr "ДИВ. ТАКОЖ"

#. type: Plain text
#: dig.1:832
msgid "B<delv>(1), B<host>(1), B<named>(8), B<dnssec-keygen>(8), RFC 1035\\&."
msgstr "B<delv>(1), B<host>(1), B<named>(8), B<dnssec-keygen>(8), RFC 1035\\&."

#. type: SH
#: dig.1:832
#, no-wrap
msgid "BUGS"
msgstr "ВАДИ"

#. type: Plain text
#: dig.1:835
msgid "There are probably too many query options\\&."
msgstr "Ймовірно, забагато параметрів запитів\\&."

#. type: SH
#: dig.1:835 host.1:266 nslookup.1:295
#, no-wrap
msgid "AUTHOR"
msgstr "АВТОР"

#. type: Plain text
#: dig.1:838 host.1:269 nslookup.1:298
