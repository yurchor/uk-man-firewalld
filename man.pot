"        I<forward-port1>\n"
"        \\&.\\&.\n"
"  icmp-blocks: I<icmp-type1> \\&.\\&.\n"
"  rich rules:\n"
"        I<rich-rule1>\n"
"        \\&.\\&.\n"
"\\&.\\&.\n"
msgstr ""

