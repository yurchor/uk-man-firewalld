#. type: SS
#: ./firewall-cmd.1:315
#, no-wrap
msgid "Options to Adapt and Query Zones and Policies"
msgstr ""

#. type: Plain text
#: ./firewall-cmd.1:323
msgid ""
"Options in this section affect only one particular zone or policy\\&. If "
"used with B<--zone>=I<zone> or B<--policy>=I<policy> option, they affect the "
"specified zone or policy\\&. If both options are omitted, they affect the "
"default zone (see B<--get-default-zone>)\\&."
msgstr ""

#. type: Plain text
#: ./firewall-cmd.1:325
msgid "[B<--permanent>] [B<--zone>=I<zone>] [B<--policy>=I<policy>] B<--list-all>"
msgstr ""

#. type: Plain text
#: ./firewall-cmd.1:327
msgid "List everything added or enabled\\&."
msgstr ""

#. type: Plain text
#: ./firewall-cmd.1:330
msgid "B<--permanent> [B<--zone>=I<zone>] [B<--policy>=I<policy>] B<--get-target>"
msgstr ""

#. type: Plain text
#: ./firewall-cmd.1:332
msgid "Get the target\\&."
msgstr ""

#. type: Plain text
#: ./firewall-cmd.1:335
msgid ""
"B<--permanent> [B<--zone>=I<zone>] [B<--policy>=I<policy>] "
"B<--set-target>=I<target>"
msgstr ""

#. type: Plain text
#: ./firewall-cmd.1:337
msgid "Set the target\\&."
msgstr ""

#. type: Plain text
#: ./firewall-cmd.1:345
msgid "For zones I<target> is one of: I<default>, I<ACCEPT>, I<DROP>, I<REJECT>"
msgstr ""

#. type: Plain text
#: ./firewall-cmd.1:353
msgid "For policies I<target> is one of: I<CONTINUE>, I<ACCEPT>, I<DROP>, I<REJECT>"
msgstr ""

#. type: Plain text
#: ./firewall-cmd.1:357
msgid ""
"I<default> is similar to I<REJECT>, but it implicitly allows ICMP "
"packets\\&."
msgstr ""

#. type: Plain text
#: ./firewall-cmd.1:360
msgid ""
"B<--permanent> [B<--zone>=I<zone>] [B<--policy>=I<policy>] "
"B<--set-description>=I<description>"
msgstr ""

#. type: Plain text
#: ./firewall-cmd.1:362
msgid "Set description\\&."
msgstr ""

