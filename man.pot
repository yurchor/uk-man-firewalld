"  forward-ports:\n"
"        I<forward-port1>\n"
"        \\&.\\&.\n"
"  source-ports: I<source-port1> \\&.\\&.\n"
"  icmp-blocks: I<icmp-type1> \\&.\\&.\n"
"  rich rules:\n"
"        I<rich-rule1>\n"
"        \\&.\\&.\n"
msgstr ""

#. type: Plain text
#: ./firewall-cmd.1:224
msgid "[B<--permanent>] B<--list-all-zones>"
msgstr ""

#. type: Plain text
#: ./firewall-cmd.1:226
msgid "List everything added for or enabled in all zones\\&. The output format is:"
